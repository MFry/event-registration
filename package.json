{
  "name": "event-form",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "muicss": "^0.9.39-rc1",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
