    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.4.0",
    "node-sass-chokidar": "^1.2.2",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"]
